        <registro CD_CGC_CLIENTE="08850962000222" CD_CLASSIF_RELAT="CATE" CD_CLASSIF_RELAT_PAIS="" CD_CLASSIF_RELAT_WHEB="WATE" CD_PAIS="" CD_RELATORIO="4192" CD_RELATORIO_PAIS="" CD_RELATORIO_WHEB="11100" CD_VERSION="31047" DS_ACTION_NAME="" DS_COR_FUNDO="clWhite" DS_PROCEDURE="" DS_REPORT_TITLE_CUSTOM="" DT_ATUALIZACAO="08/12/2022 17:50:49" DT_LAST_MODIFICATION="08/12/2022 17:50:49" IE_AJUSTAR_TAMANHO="" IE_BORDA_DIR="S" IE_BORDA_ESQ="S" IE_BORDA_INF="S" IE_BORDA_SUP="S" IE_CHAMADA_DIRETA="" IE_ETIQUETA="N" IE_FILTRO_EXCEL="S" IE_FILTRO_HTML="S" IE_FILTRO_TEXTO="S" IE_FILTRO_WORD="S" IE_GERAR_BASE="S" IE_GERAR_RELATORIO="S" IE_IMPRIME_VAZIO="S" IE_ORIENTACAO="R" IE_TIPO_PAPEL="A4" IE_TIPO_RELATORIO="" NM_TABELA="" NM_USUARIO="rafael.ramalho" NR_SEQ_MOD_IMPL="" NR_SEQ_MODULO="13" NR_SEQ_ORDEM_SERV="" NR_SEQUENCIA="2879216" NR_VERSAO="" QT_ALTURA="" QT_COLUNA="1" QT_ESPACO_COLUNA="0" QT_LARGURA="" QT_MARGEM_DIR="10" QT_MARGEM_ESQ="10" QT_MARGEM_INF="20" QT_MARGEM_SUP="10" >
          <DS_SQL><![CDATA[SELECT
    'VISITA SETOR' TP_VISITA,
    obter_nome_setor(AV.CD_SETOR_ATENDIMENTO) DESTINO,
    AV.NR_SEQ_CONTROLE NR_CONTROLE,
    OBTER_NOME_pf(AV.CD_PESSOA_FISICA) NM_VISITANTE,
    AV.DT_ENTRADA,
    AV.DT_SAIDA
FROM
    ATENDIMENTO_VISITA AV
    LEFT JOIN SETOR_ATENDIMENTO sa ON av.cd_setor_atendimento = sa.cd_setor_atendimento
WHERE
    (AV.DT_ENTRADA BETWEEN :DT_ENTRADA AND fim_dia(:DT_SAIDA) or av.dt_saida BETWEEN :DT_ENTRADA AND fim_dia(:DT_SAIDA))
    and	sa.cd_estabelecimento = :cd_estabelecimento
    and av.cd_setor_atendimento is NOT null
    and	((av.cd_setor_atendimento = :cd_setor_atendimento) or (:cd_setor_atendimento = 0))
union
SELECT
    'VISITA PACIENTE' TP_VISITA,
    NVL(obter_nome_setor(AV.CD_SETOR_ATEND), OBTER_UNIDADE_ATENDIMENTO(AV.NR_ATENDIMENTO, 'IAA', 'S')) DESTINO,
    AV.NR_SEQ_CONTROLE NR_CONTROLE,
    OBTER_NOME_pf(AV.CD_PESSOA_FISICA) NM_VISITANTE,
    AV.DT_ENTRADA,
    AV.DT_SAIDA
FROM
    ATENDIMENTO_VISITA AV
    LEFT JOIN ATENDIMENTO_PACIENTE AP ON AV.NR_ATENDIMENTO = AP.NR_ATENDIMENTO
WHERE
    (AV.DT_ENTRADA BETWEEN :DT_ENTRADA AND fim_dia(:DT_SAIDA) or av.dt_saida BETWEEN :DT_ENTRADA AND fim_dia(:DT_SAIDA))
    and	ap.cd_estabelecimento = :cd_estabelecimento
    AND AV.NR_ATENDIMENTO IS NOT NULL
    and	((AV.CD_SETOR_ATEND = :cd_setor_atendimento) or (OBTER_UNIDADE_ATENDIMENTO(AV.NR_ATENDIMENTO, 'IAA', 'CS') = :cd_setor_atendimento) or (:cd_setor_atendimento = 0))
UNION
select
    'ACOMPANHANTE PACIENTE' TP_VISITA,
    OBTER_UNIDADE_ATENDIMENTO(AA.NR_ATENDIMENTO, 'IAA', 'S') DESTINO,
    to_number(AA.NR_CONTROLE) nr_controle,
    OBTER_NOME_pf(AA.CD_PESSOA_FISICA) NM_VISITANTE,
    AA.DT_ACOMPANHANTE DT_ENTRADA,
    AA.DT_SAIDA
FROM
    atendimento_acompanhante aa
    LEFT JOIN ATENDIMENTO_PACIENTE AP ON AA.NR_ATENDIMENTO = AP.NR_ATENDIMENTO
WHERE
    (AA.DT_ACOMPANHANTE BETWEEN :DT_ENTRADA AND fim_dia(:DT_SAIDA) or AA.dt_saida BETWEEN :DT_ENTRADA AND fim_dia(:DT_SAIDA))
    and	ap.cd_estabelecimento = :cd_estabelecimento
    AND AA.NR_ATENDIMENTO IS NOT NULL
    and	((OBTER_UNIDADE_ATENDIMENTO(AA.NR_ATENDIMENTO, 'IAA', 'CS') = :cd_setor_atendimento) or (:cd_setor_atendimento = 0))
ORDER BY
    DESTINO,
    DT_ENTRADA]]></DS_SQL>
>
